{
  "name": "Energy Consume Notify Event Popup",
  "description": "Demonstrates usage and features of the event page by reminding user to consume energy",
  "version": "0.5",
  "manifest_version": 2,
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Energy Consume Notify",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/energy_notify16.png",
    "32": "img/energy_notify32.png",
    "48": "img/energy_notify48.png",
    "128": "img/energy_notify128.png"
  }
}
